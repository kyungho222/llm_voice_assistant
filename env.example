# LLM 음성 비서 환경 변수 설정
# 이 파일을 복사하여 .env 파일로 만드세요

# Gemini API Key (Google AI Studio에서 발급)
# https://makersuite.google.com/app/apikey 에서 발급받으세요
GEMINI_API_KEY=your-gemini-api-key

# Google Cloud Speech-to-Text 인증 파일 경로
# Google Cloud Console에서 서비스 계정 키를 다운로드하여 설정하세요
GOOGLE_CREDENTIALS_PATH=backend/teak-mix-466716-h0-3fc9e37b08ce.json

# 서버 설정
FLASK_ENV=development
FLASK_DEBUG=True

# 포트 설정 (기본값: 8000)
PORT=8000

# 호스트 설정 (기본값: 0.0.0.0)
HOST=0.0.0.0 